package testprogramss;



public class CheckPrime {
	public static void main(String[] args) {
		String a = args[0];
		int al =Integer.parseInt(a);
		
		int sum = 0;
		for (int i = 0; i < 1; i++) {
			sum = al% 2;
			if (al <= 0) {
				System.out.println("invalid");
			} else if (sum == 0 || al == 1) {
				System.out.println(a + " is not prime ");
			}

			else
				System.out.println(a + "  is prime ");
		}
	}

}
