package testprogramss;

public class CheckPrime {
	public static void main(String[] args) {
		int a = 0;
		int sum = 0;

		sum = a % 2;
		if (a <= 0 && a == 1) {
			System.out.println("invalid");
		} else if (sum == 0 || a == 1) {

			System.out.println(a + " is not prime ");
		}

		else
			System.out.println(a + "  is prime ");

	}
}
