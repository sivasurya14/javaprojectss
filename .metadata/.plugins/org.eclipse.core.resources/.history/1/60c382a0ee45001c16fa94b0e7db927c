public class CheckPrime {
	public static void main(String[] args) {
		String a = args[0];
		List<String> no= new ArrayList<>();
		no=Arrays.asList(a.split(","));
		List<Integer> al = new ArrayList<>();
		for(String sd:no) {
			int number=Integer.parseInt(sd);
			al.add(number);
			
		}
		
	
		System.out.println(al);

