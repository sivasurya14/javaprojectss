package testprogramss;

public class MedianOfTheSeriesEx {

	public static void main(String[] args) {

		int[] a = { 4, 2, 5, 3, 8, 6 };
		int temp = 0;
		for (int i = 0; i < a.length; i++) {

			for (int j = 1+1; j < a.length; j++) {

				if (a[i] > a[j]) {
					temp = a[i];
					a[i] = a[j];
					a[j] = temp;
				}
				}
			}
		for(int i=0; i<a.length;i++) {
			
			System.out.print(a[i]+" ");
		}
	}
}
