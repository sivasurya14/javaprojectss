package testprogramss;

public class TestFactorial {
public static void main(String[] args) {
	
	int a=5;
	int sum=0;
	if(a<0) {
		System.out.println("invalid number");
	}
	else if(a>100) {
		System.out.println("out of range ");
	}
		else  {
			sum=0;
			for(int i=0; i<a; i++) {
				
			sum=i+1*i;	
		}
		System.out.println(sum);
	}
	
}
}
		
		
	
		



