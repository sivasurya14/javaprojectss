package day3class;

import java.util.ArrayList;
import java.util.List;

public class ConcurrentEx {
public static void main(String[] args) {
	
	List<String> list = new ArrayList<>();
	
	
	list.add("siva");
	list.add("dinesh");
	list.add("guru");
	
	for(String x : list) {
		
		System.out.println(x);
		list.remove(1);
	}
	
	
}
}
