package day2Program;

public class SortingArrayWithoutUsermethod {
	
public static void main(String[] args) {
	
	int arr[] = {23,10,13,21,22,13,14,10};
	
	System.out.println();
	 int temp=0;
	for(int i=0;i<arr.length;i++) {
      for(int j=i+1;j<arr.length;j++) {
    	  if(arr[i]<arr[j]) {  		  
    		  temp=arr[i];
    		  arr[i]=arr[j];
    		  arr[j]=temp;
   	  }	  
      }
      System.out.println(arr[i]);
	}	
}
}
